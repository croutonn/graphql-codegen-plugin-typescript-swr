{
  "name": "graphql-codegen-plugin-typescript-swr",
  "version": "0.1.0",
  "description": "A GraphQL code generator plug-in that automatically generates utility functions for SWR.",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/croutonn/graphql-codegen-plugin-typescript-swr",
  "license": "MIT",
  "keywords": [
    "graphql",
    "codegen",
    "typescript",
    "ts",
    "swr",
    "stale-while-revalidate",
    "react"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.main.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:integrate": "jest --no-watchman",
    "watch:build": "tsc -p tsconfig.json -w"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^1.17.9",
    "@graphql-codegen/visitor-plugin-common": "^1.17.15",
    "auto-bind": "^4.0.0",
    "graphql": "^15.3.0",
    "graphql-request": "^3.1.0",
    "pascal-case": "^3.1.1",
    "swr": "^0.3.5",
    "tslib": "^2.0.3"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0",
    "graphql-tag": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "^1.17.7",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-graphql-request": "^2.0.1",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.5.3",
    "jest-docblock": "^26.0.0",
    "jest-junit": "^12.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "graphql": "15.3.0",
    "graphql-language-service-interface": "2.4.0",
    "**/@types/graphql-upload/graphql": "^15.0.0"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "LICENSE",
    "README.md"
  ]
}
